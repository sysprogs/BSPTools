<?xml version="1.0"?>
<EmbeddedProjectSample xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>LEDBlink</Name>
	<Description>This is a very basic sample project - a classical blinking LED example.</Description>
	<ConfigurableProperties>
	<PropertyGroups>
	<PropertyGroup>
	  <Properties>
		<PropertyEntry xsi:type="Enumerated">
		  <Name>LED Port Group</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.LEDPORT</UniqueID>
		  <Description>Specifies the port group where the LED is connected</Description>
		  <SuggestionList>
			<Suggestion><InternalValue>P1</InternalValue></Suggestion>
			<Suggestion><InternalValue>P2</InternalValue></Suggestion>
			<Suggestion><InternalValue>P3</InternalValue></Suggestion>
			<Suggestion><InternalValue>P4</InternalValue></Suggestion>
			<Suggestion><InternalValue>P5</InternalValue></Suggestion>
			<Suggestion><InternalValue>P6</InternalValue></Suggestion>
			<Suggestion><InternalValue>P7</InternalValue></Suggestion>
			<Suggestion><InternalValue>P8</InternalValue></Suggestion>
			<Suggestion><InternalValue>P9</InternalValue></Suggestion>
			<Suggestion><InternalValue>P10</InternalValue></Suggestion>
			<Suggestion><InternalValue>P11</InternalValue></Suggestion>
			<Suggestion><InternalValue>PJ</InternalValue></Suggestion>
		  </SuggestionList>
		  <DefaultEntryIndex>0</DefaultEntryIndex>
		  <AllowFreeEntry>false</AllowFreeEntry>
		</PropertyEntry>
		<PropertyEntry xsi:type="Integral">
		  <Name>LED Port Number</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.LEDBIT</UniqueID>
		  <Description>Specifies the port number of the LED port</Description>
		  <DefaultValue>0</DefaultValue>
		  <MinValue>0</MinValue>
		  <MaxValue>15</MaxValue>
		</PropertyEntry>
		<PropertyEntry xsi:type="Integral">
		  <Name>Delay (cycles)</Name>
		  <UniqueID>com.sysprogs.examples.ledblink.DELAY</UniqueID>
		  <Description>Specifies the delay (in cycles) between LED toggling events</Description>
		  <DefaultValue>10000</DefaultValue>
		</PropertyEntry>
	  </Properties>
	  <CollapsedByDefault>false</CollapsedByDefault>
	</PropertyGroup>
	</PropertyGroups>
	</ConfigurableProperties>
</EmbeddedProjectSample>