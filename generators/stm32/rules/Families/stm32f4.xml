<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>STM32F4</Name>
	<DeviceRegex>^STM32F4.*</DeviceRegex>
	<FamilySubdirectory>STM32F4xxxx</FamilySubdirectory>
	<PrimaryHeaderDir>$$BSPGEN:INPUT_DIR$$\STM32Cube_FW_F4_V1.14.0\Drivers\CMSIS\Device\ST\STM32F4xx\Include</PrimaryHeaderDir>
	<StartupFileDir>$$BSPGEN:INPUT_DIR$$\STM32Cube_FW_F4_V1.14.0\Drivers\CMSIS\Device\ST\STM32F4xx\Source\Templates\gcc</StartupFileDir>
	<Subfamilies>
		<MCUClassifier>
			<VariableName>com.sysprogs.stm32.hal_device_family</VariableName>
			<Required>true</Required>
			<IsPrimary>true</IsPrimary>
			<AutoOptions>STM32F405xx;STM32F415xx;STM32F407xx;STM32F417xx;STM32F427xx;STM32F437xx;STM32F429xx;STM32F439xx;STM32F446xx;STM32F469xx;STM32F479xx;STM32F412Rx;STM32F412Vx;STM32F412Rx;STM32F412Zx;STM32F423xx</AutoOptions>
			<Options>
			<Option>
					<Value>STM32F423xx</Value>
					<Regex>STM32F423..</Regex>
			</Option>

			<Option>
					<Value>STM32F412Cx</Value>
					<Regex>STM32F412C.</Regex>
			</Option>
			<Option>
					<Value>STM32F413xx</Value>
					<Regex>STM32F413..</Regex>
			</Option>
			<Option>
					<Value>STM32F412Rx</Value>
					<Regex>STM32F412R.</Regex>
			</Option>
						<Option>
					<Value>STM32F412Vx</Value>
					<Regex>STM32F412V.</Regex>
			</Option>
						<Option>
					<Value>STM32F412Zx</Value>
					<Regex>STM32F412Z.</Regex>
			</Option>

				<Option>
					<Value>STM32F410Cx</Value>
					<Regex>STM32F410C8|STM32F410CB</Regex>
				</Option>
			<Option>
					<Value>STM32F410Tx</Value>
					<Regex>STM32F410T8|STM32F410TB</Regex>
				</Option>
			<Option>
					<Value>STM32F410Rx</Value>
					<Regex>STM32F410R8|STM32F410RB</Regex>
				</Option>
				<Option>
					<Value>STM32F401xC</Value>
					<Regex>STM32F401.B|STM32F401.C</Regex>
				</Option>
				<Option>
					<Value>STM32F401xE</Value>
					<Regex>STM32F401.D|STM32F401.E</Regex>
				</Option>
				<Option>
					<Value>STM32F411xE</Value>
					<Regex>STM32F411..</Regex>
				</Option>
			</Options>
		</MCUClassifier>
		<MCUClassifier>
			<VariableName>com.sysprogs.stm32.legacy_device_family</VariableName>
			<Required>true</Required>
			<UnsupportedMCUs>STM32F479.*</UnsupportedMCUs>
			<AutoOptions>STM32F401xx;STM32F446xx;STM32F469xx;STM32F410xx</AutoOptions>
			<Options>
						
				<Option>
					<Value>STM32F413_423xx</Value>
					<Regex>STM32F413..</Regex>
				</Option>
				<Option>
					<Value>STM32F413_423xx</Value>
					<Regex>STM32F423..</Regex>
				</Option>
						
				<Option>
					<Value>STM32F412xG</Value>
					<Regex>STM32F412..</Regex>
				</Option>
				<Option>
					<Value>STM32F40_41xxx</Value>
					<Regex>STM32F4[01]5..|STM32F4[01]7..</Regex>
				</Option>
				<Option>
					<Value>STM32F427_437xx</Value>
					<Regex>STM32F4[23]7..</Regex>
				</Option>
				<Option>
					<Value>STM32F429_439xx</Value>
					<Regex>STM32F4[23]9..</Regex>
				</Option>
				<Option>
					<Value>STM32F411xE</Value>
					<Regex>STM32F411..</Regex>
				</Option>
				<Option>
					<Value>STM32F469_479xx</Value>
					<Regex>STM32F4[67]9..</Regex>
				</Option>
			</Options>
		</MCUClassifier>
	</Subfamilies>
	<CompilationFlags>
		<IncludeDirectories>
			<string>.</string>
		</IncludeDirectories>
	</CompilationFlags>
	<CoreFramework>
		<CopyJobs>
		</CopyJobs>
	</CoreFramework>
	<AdditionalFrameworks>
		<Framework>
			<Name>STM32F4Cube HAL</Name>
			<ID>com.sysprogs.arm.stm32.f4_hal</ID>
			<ClassID>com.sysprogs.arm.stm32.hal</ClassID>
			<IncompatibleFrameworks>
				<string>com.sysprogs.arm.stm32.f4_stdperiph</string>
			</IncompatibleFrameworks>
			<ProjectFolderName>HAL</ProjectFolderName>
			<DefaultEnabled>true</DefaultEnabled>
			<CopyJobs>
				<CopyJob>
					<SourceFolder>$$BSPGEN:INPUT_DIR$$\STM32Cube_FW_F4_V1.14.0\Drivers\STM32F4xx_HAL_Driver</SourceFolder>
					<FilesToCopy>*.c;*.h</FilesToCopy>
					<ProjectInclusionMask>-*_template.?;*</ProjectInclusionMask>
				</CopyJob>
				<CopyJob>
					<SourceFolder>$$BSPGEN:INPUT_DIR$$\STM32Cube_FW_F4_V1.14.0\Drivers\CMSIS</SourceFolder>
					<FilesToCopy>-DSP_Lib\*;*.h;*.c</FilesToCopy>
					<TargetFolder>CMSIS_HAL</TargetFolder>
					<PreprocessorMacros>stm32_$$com.sysprogs.bspoptions.primary_memory$$_layout;$$com.sysprogs.stm32.hal_device_family$$</PreprocessorMacros>
					<ProjectInclusionMask>-*Include\stm32f4????.h*;-*system_*.c;*</ProjectInclusionMask>
					<AutoIncludeMask>-RTOS\Template\*;*.h</AutoIncludeMask>		
					<Patches>
						<Patch xsi:type="InsertLines">
							<FilePath>Device\ST\STM32f4xx\Source\Templates\system_stm32f4xx.c</FilePath>
							<AfterLine>#include "stm32f4xx.h"</AfterLine>
							<InsertedLines>
								<string>#include &lt;stm32f4xx_hal.h&gt;</string>
							</InsertedLines>
						</Patch>					
					</Patches>
				</CopyJob>
			</CopyJobs>			
		</Framework>
		<Framework>
			<Name>STM32F4 Default System Init File</Name>
			<ID>com.sysprogs.arm.stm32.f4_systeminit</ID>
			<ClassID>com.sysprogs.arm.stm32.hal_systeminit</ClassID>
			<RequiredFrameworks>
				<string>com.sysprogs.arm.stm32.f4_hal</string>
			</RequiredFrameworks>
			<ProjectFolderName>SystemInit</ProjectFolderName>
			<DefaultEnabled>true</DefaultEnabled>
			<CopyJobs>
				<CopyJob>
					<AlreadyCopied>true</AlreadyCopied>
					<TargetFolder>CMSIS_HAL</TargetFolder>
					<SourceFolder>$$BSPGEN:INPUT_DIR$$\STM32Cube_FW_F4_V1.14.0\Drivers\CMSIS</SourceFolder>
					<FilesToCopy>*system_stm*.c</FilesToCopy>
				</CopyJob>
			</CopyJobs>			
		</Framework>
		<Framework>
			<Name>STM32F4 Legacy Peripheral Library</Name>
			<ID>com.sysprogs.arm.stm32.f4_stdperiph</ID>
			<IncompatibleFrameworks>
				<string>com.sysprogs.arm.stm32.f4_hal</string>
			</IncompatibleFrameworks>
			<ProjectFolderName>StdPeriph</ProjectFolderName>
			<DefaultEnabled>false</DefaultEnabled>
			<CopyJobs>
				<CopyJob>
					<SourceFolder>$$BSPGEN:INPUT_DIR$$\STM32F4xx_DSP_StdPeriph_Lib_V1.8.0\Libraries\STM32F4xx_StdPeriph_Driver</SourceFolder>
					<FilesToCopy>*.c;*.h</FilesToCopy>
					<ProjectInclusionMask>-*stm32f4xx_fmc.c;*</ProjectInclusionMask>
					<SimpleFileConditions>
						<string>\\stm32f4xx_fsmc.c: $$com.sysprogs.stm32.legacy_device_family$$ == STM32F40_41xxx</string>
						<string>\\stm32f4xx_qspi.c: $$com.sysprogs.stm32.legacy_device_family$$ != STM32F446xx</string>
					</SimpleFileConditions>
					<Patches>
				
					<Patch xsi:type="InsertLines">
                        <FilePath>src\stm32f4xx_dsi.c</FilePath>
                        <AfterLine>#include "stm32f4xx_dsi.h"</AfterLine>
                        <InsertedLines>
                             <string>#include "stm32f4xx_rcc.h"</string>
                        </InsertedLines>
					</Patch>
					<Patch xsi:type="InsertLines">
                        <FilePath>src\stm32f4xx_lptim.c</FilePath>
                        <AfterLine>#include "stm32f4xx_lptim.h"</AfterLine>
                        <InsertedLines>
                             <string>#include "stm32f4xx_rcc.h"</string>
                        </InsertedLines>
					</Patch>
					<Patch xsi:type="InsertLines">
                        <FilePath>src\stm32f4xx_qspi.c</FilePath>
                        <AfterLine>#include "stm32f4xx_qspi.h"</AfterLine>
                        <InsertedLines>
                             <string>#include "stm32f4xx_rcc.h"</string>
                        </InsertedLines>
					</Patch>
				
				</Patches>
				</CopyJob>
				<CopyJob>
					<SourceFolder>$$BSPGEN:INPUT_DIR$$\STM32F4xx_DSP_StdPeriph_Lib_V1.8.0\Libraries\CMSIS</SourceFolder>
					<TargetFolder>CMSIS_StdPeriph</TargetFolder>
					<FilesToCopy>-DSP_Lib\*;*.h;*.c</FilesToCopy>
					<PreprocessorMacros>stm32_$$com.sysprogs.bspoptions.primary_memory$$_layout;$$com.sysprogs.stm32.legacy_device_family$$</PreprocessorMacros>
					<ProjectInclusionMask>-*Include\stm32f4????.h*;-*system_*.c;*</ProjectInclusionMask>
					<Patches>
						<Patch xsi:type="InsertLines">
							<FilePath>Device\ST\STM32F4xx\Include\stm32f4xx.h</FilePath>
							<AfterLine>#define __STM32F4xx_H</AfterLine>
							<InsertedLines>
								<string></string>
								<string>#define assert_param(expr)</string>
							</InsertedLines>
						</Patch>
					</Patches>
				</CopyJob>
			</CopyJobs>			
		</Framework>
	</AdditionalFrameworks>
	<SmartSamples>
		<SmartSample>
			<SourceFolder>$$BSPGEN:RULES_DIR$$\samples\LEDBlink_STM32F4X</SourceFolder>
			<DestinationFolder>samples\LEDBlink_STM32F4X</DestinationFolder>
			<AdditionalSources>
				<string>$$SYS:BSP_ROOT$$/STM32F4xxxx/CMSIS_StdPeriph/Device/ST/STM32F4xx/Source/Templates/system_stm32f4xx.c</string>
			</AdditionalSources>
		</SmartSample>
		<SmartSample>
			<SourceFolder>$$BSPGEN:RULES_DIR$$\samples\USB_CDC_STM32F4X</SourceFolder>
			<DestinationFolder>samples\USB_CDC_STM32F4X</DestinationFolder>
			<AdditionalSources>
				<string>$$SYS:BSP_ROOT$$/$$com.sysprogs.bspoptions.stm32.hal_system_file$$</string>
				<string>$$SYS:BSP_ROOT$$/$$com.sysprogs.bspoptions.stm32.hal_config_template_file$$=>$$com.sysprogs.stm32.hal_header_prefix$$_hal_conf.h</string>
			</AdditionalSources>
		</SmartSample>
			
	</SmartSamples>
      <AdditionalSystemVars>
        <SysVarEntry>
          <Key>com.sysprogs.stm32.hal_header_prefix</Key>
          <Value>stm32f4xx</Value>
        </SysVarEntry>
        <SysVarEntry>
          <Key>com.sysprogs.bspoptions.stm32.hal_system_file</Key>
          <Value>STM32F4xxxx/CMSIS_HAL/Device/ST/STM32F4xx/Source/Templates/system_stm32f4xx.c</Value>
        </SysVarEntry>
        <SysVarEntry>
          <Key>com.sysprogs.bspoptions.stm32.hal_config_template_file</Key>
          <Value>STM32F4xxxx/STM32F4xx_HAL_Driver/Inc/stm32f4xx_hal_conf_template.h</Value>
        </SysVarEntry>
      </AdditionalSystemVars>
</FamilyDefinition>
