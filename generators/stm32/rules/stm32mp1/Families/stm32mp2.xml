<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>STM32MP2</Name>
	<DeviceRegex>^STM32MP2.*_M33</DeviceRegex>
	<FamilySubdirectory>STM32MP2xxxx</FamilySubdirectory>
	<PrimaryHeaderDir>$$STM32:MP2_DIR$$\Drivers\CMSIS\Device\ST\STM32MP2xx\Include</PrimaryHeaderDir>
	<StartupFileDir>$$STM32:MP2_DIR$$\Drivers\CMSIS\Device\ST\STM32MP2xx\Source\Templates\gcc</StartupFileDir>
	<CompilationFlags>
		<PreprocessorMacros>
			<string>CORE_CM33</string>
			<string>INITIALIZE_SP_AT_RESET</string>
		</PreprocessorMacros>
		<IncludeDirectories>
			<string>.</string>
		</IncludeDirectories>
	</CompilationFlags>
	<CoreFramework>
		<CopyJobs>
			<CopyJob>
				<SourceFolder>$$STM32:MP2_DIR$$\Projects\STM32MP257F-DK\Applications\FreeRTOS\FreeRTOS_ThreadCreation\CM33\NonSecure\Core\Inc</SourceFolder>
				<TargetFolder>Templates</TargetFolder>
				<FilesToCopy>stm32mp2xx_hal_conf.h</FilesToCopy>
				<ProjectInclusionMask>-*</ProjectInclusionMask>
				<AutoIncludeMask/>
				<ExcludeFromVendorSampleMapping>true</ExcludeFromVendorSampleMapping>
			</CopyJob>
		</CopyJobs>
	</CoreFramework>
	<AdditionalFrameworks>
		<Framework>
			<Name>DSP Functions</Name>
			<ID>com.sysprogs.arm.stm32.dsp.STM32MP2</ID>
			<ClassID>com.sysprogs.arm.stm32.dsp</ClassID>
			<ProjectFolderName>DSP</ProjectFolderName>
			<DefaultEnabled>false</DefaultEnabled>
			<RequiredFrameworks>
			</RequiredFrameworks>
			<CopyJobs>
				<CopyJob>
					<SourceFolder>$$STM32:MP2_DIR$$\Drivers\CMSIS\DSP</SourceFolder>
					<TargetFolder>DSP</TargetFolder>
					<FilesToCopy>*</FilesToCopy>
					<ProjectInclusionMask>-DSP_Lib_TestSuite\*;-Examples\*;-*template_*;*.c;*.h</ProjectInclusionMask>
				</CopyJob>
			</CopyJobs>
		</Framework>
	</AdditionalFrameworks>
	<SmartSamples>
	</SmartSamples>
      <AdditionalSystemVars>
        <SysVarEntry>
          <Key>com.sysprogs.stm32.hal_header_prefix</Key>
          <Value>STM32MP2xx</Value>
        </SysVarEntry>
        <SysVarEntry>
          <Key>com.sysprogs.bspoptions.stm32.hal_system_file</Key>
          <Value>STM32MP2xxxx/CMSIS_HAL/Device/ST/STM32MP2xx/Source/Templates/system_stm32mp2xx_m33.c</Value>
        </SysVarEntry>
        <SysVarEntry>
          <Key>com.sysprogs.bspoptions.stm32.hal_config_template_file</Key>
          <Value>STM32MP2xxxx/Templates/stm32mp2xx_hal_conf.h</Value>
        </SysVarEntry>
      </AdditionalSystemVars>
</FamilyDefinition>
