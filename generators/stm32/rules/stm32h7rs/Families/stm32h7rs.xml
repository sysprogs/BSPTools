<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>STM32H7RS</Name>
	<DeviceRegex>^STM32H7[RS].*[^M4]$</DeviceRegex>
	<FamilySubdirectory>STM32H7RSxx</FamilySubdirectory>
	<PrimaryHeaderDir>$$STM32:H7RS_DIR$$\Drivers\CMSIS\Device\ST\STM32H7RSxx\Include</PrimaryHeaderDir>
	<StartupFileDir>$$STM32:H7RS_DIR$$\Drivers\CMSIS\Device\ST\STM32H7RSxx\Source\Templates\gcc</StartupFileDir>
	<Subfamilies>
	</Subfamilies>
	<CompilationFlags>
	<PreprocessorMacros>
		<string>CORE_CM7</string>
	</PreprocessorMacros>
	<IncludeDirectories>
		<string>.</string>
	</IncludeDirectories>
	</CompilationFlags>
	<CoreFramework>
		<CopyJobs>
		</CopyJobs>
	</CoreFramework>
	<AdditionalFrameworks>
		<Framework>
			<Name>DSP Functions</Name>
			<ID>com.sysprogs.arm.stm32.dsp.STM32H7RS</ID>
			<ClassID>com.sysprogs.arm.stm32.dsp</ClassID>
			<ProjectFolderName>DSP</ProjectFolderName>
			<DefaultEnabled>false</DefaultEnabled>
			<RequiredFrameworks>
			</RequiredFrameworks>
			<CopyJobs>
				<CopyJob>
					<SourceFolder>$$STM32:H7RS_DIR$$\Drivers\CMSIS\DSP</SourceFolder>
					<TargetFolder>DSP</TargetFolder>
					<FilesToCopy>*</FilesToCopy>
					<ProjectInclusionMask>-DSP_Lib_TestSuite\*;-Examples\*;-*template_*;*.c;*.h</ProjectInclusionMask>
				</CopyJob>
			</CopyJobs>
		</Framework>
	</AdditionalFrameworks>
	<AdditionalFrameworkTemplates>
		<FrameworkTemplate>
			<RangeFromDir>$$STM32:H7RS_DIR$$\Drivers\BSP</RangeFromDir>
			<RangeFilters>-Components;*</RangeFilters>
			<Template>
				<Name>$$BSPGEN:FRAMEWORK$$ drivers</Name>
				<ID>com.sysprogs.arm.stm32.bspdrv.h7rs_$$BSPGEN:FRAMEWORK_LOWER$$</ID>
				<ClassID>com.sysprogs.arm.stm32.bspdrv.$$BSPGEN:FRAMEWORK_LOWER$$</ClassID>
				<Filter>STM32H7[RS].*</Filter>
				<RequiredFrameworks>
					<string>com.sysprogs.arm.stm32.STM32H7RS_hal</string>
				</RequiredFrameworks>
				<ProjectFolderName>$$BSPGEN:FRAMEWORK$$</ProjectFolderName>
				<DefaultEnabled>false</DefaultEnabled>
				<CopyJobs>
					<CopyJob>
						<SourceFolder>$$STM32:H7RS_DIR$$\Drivers\BSP\$$BSPGEN:FRAMEWORK$$</SourceFolder>
						<TargetFolder>BSP\$$BSPGEN:FRAMEWORK$$</TargetFolder>
						<FilesToCopy>*</FilesToCopy>
						<ProjectInclusionMask>*.c;*.h</ProjectInclusionMask>
						<SmartPropertyGroup>com.sysprogs.bspoptions.$$BSPGEN:FRAMEWORK_LOWER$$.|$$BSPGEN:FRAMEWORK$$</SmartPropertyGroup>
						<AutoSmartFileConditions>
							<string>^[^_]+_[^_]+_(***)\.[ch]$</string>
						</AutoSmartFileConditions>
					</CopyJob>
				</CopyJobs>			
			</Template>
		</FrameworkTemplate>
		<FrameworkTemplate>
			<RangeFromDir>$$STM32:H7RS_DIR$$\Drivers\BSP\Components</RangeFromDir>
			<RangeFilters>-Common;*</RangeFilters>
			<Template>
				<Name>$$BSPGEN:FRAMEWORK$$ Driver</Name>
				<ID>com.sysprogs.arm.stm32.bspdrv.h7rs_$$BSPGEN:FRAMEWORK$$</ID>
				<ClassID>com.sysprogs.arm.stm32.bspdrv.$$BSPGEN:FRAMEWORK$$</ClassID>
				<Filter>STM32H7[RS].*</Filter>
				<RequiredFrameworks>
					<string>com.sysprogs.arm.stm32.h7rs_hal</string>
				</RequiredFrameworks>
				<ProjectFolderName>$$BSPGEN:FRAMEWORK$$ Driver</ProjectFolderName>
				<DefaultEnabled>true</DefaultEnabled>
				<CopyJobs>
					<CopyJob>
						<SourceFolder>$$STM32:H7RS_DIR$$\Drivers\BSP\Components</SourceFolder>
						<TargetFolder>BSP\Components</TargetFolder>
						<AdditionalIncludeDirs>?</AdditionalIncludeDirs>
						<FilesToCopy>$$BSPGEN:FRAMEWORK$$\*;Common\*</FilesToCopy>
						<ProjectInclusionMask>*.c;*.h</ProjectInclusionMask>
					</CopyJob>
				</CopyJobs>			
			</Template>
		</FrameworkTemplate>
	</AdditionalFrameworkTemplates>
	<SmartSamples>
	</SmartSamples>
      <AdditionalSystemVars>
        <SysVarEntry>
          <Key>com.sysprogs.stm32.hal_header_prefix</Key>
          <Value>stm32h7rsxx</Value>
        </SysVarEntry>
        <SysVarEntry>
          <Key>com.sysprogs.bspoptions.stm32.hal_system_file</Key>
          <Value>STM32H7RSxx/CMSIS_HAL/Device/ST/STM32H7RSxx/Source/Templates/system_stm32h7rsxx.c</Value>
        </SysVarEntry>
        <SysVarEntry>
          <Key>com.sysprogs.bspoptions.stm32.hal_config_template_file</Key>
          <Value>STM32H7RSxx/STM32H7RSxx_HAL_Driver/Inc/STM32H7rsxx_hal_conf_template.h</Value>
        </SysVarEntry>
      </AdditionalSystemVars>
</FamilyDefinition>
